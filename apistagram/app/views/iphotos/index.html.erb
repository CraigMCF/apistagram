<h1>Listing iphotos</h1>

<table>
  <tr>
    <th>I</th>
    <th>Url</th>
    <th>Username</th>
    <th>Tag</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @iphotos.each do |iphoto| %>
  <tr>
    <td><%= iphoto.i_id %></td>
    <td><%= iphoto.url %></td>
    <td><%= iphoto.username %></td>
    <td><%= iphoto.tag_id %></td>
    <td><%= iphoto.status %></td>
    <td><%= link_to 'Show', iphoto %></td>
    <td><%= link_to 'Edit', edit_iphoto_path(iphoto) %></td>
    <td><%= link_to 'Destroy', iphoto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Iphoto', new_iphoto_path %>
